{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-var": 0,
        "indent": ["error", 4],
        "prefer-template": 0,
        "prefer-es6-class": 0,
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "no-unused-expressions": 0,
        "object-shorthand": 0,
        "react/prefer-es6-class": 0
    },
    "env": {
        "mocha": true
    }
}